<%- include('partials/header') -%>
<h1 class="welcome">Welcome, <%= user.fName %>  </h1>
<h3>My Ratings </h3>

<div class="containers"> 
    <div class="card mb-3 text-dark bg-light" style="max-width: 500px;">
        <div class="row g-0">
        <div class="col-md-4">
            <img src="..." class="img-fluid rounded-start" alt="...">
        </div>
        <div class="col-md-8">
            <div class="card-body">
            <h5 class="card-title">Card title</h5>
            <p class="card-text">Rating:</p>
            <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
            <div class="cardicon">
                <p class="card-text"><i class="fa-regular fa-star"></i></p>
                <p class="card-text"><i class="fa-solid fa-trash text-muted"></i></p>
            </div>
            <p class="card-text"><small class="text-muted">Published:</small></p>
            </div>
        </div>
        </div>
    </div>
</div>
<%- include('partials/footer') -%>